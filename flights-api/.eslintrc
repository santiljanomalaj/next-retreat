{
  "settings": {},
  "extends": [
    "airbnb-base",
    "plugin:prettier/recommended",
    "plugin:jest/recommended"
  ],
  "env": {
    "node": true,
    "jest": true
  },
  "rules": {
    "semi": [2, "never"],
    "arrow-body-style": ["error", "as-needed"],
    "no-restricted-imports": ["error"],
    "class-methods-use-this": 0,
    "eslint-disable no-restricted-syntax": 0,
    "consistent-return": 0,
    "camelcase": 0
  }
}
